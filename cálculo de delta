programa
{
	inclua biblioteca Matematica --> mat
	
	funcao inicio()
	{
		real A
		real B 
		real C
          inteiro delta
          real x1
          real x2

          
		escreva("Dgite o  valor de A : ")
		  leia(A)
		  
		escreva("Dgite o  valor de B : ")
		  leia(B)
		  
		escreva("Dgite o  valor de C : ")
		  leia(C)

		  delta = ((B*B) -4*A*C)
           escreva("O valor de delta é   : " , delta)
		  se(delta<0){
		  	escreva(" impossível calcular.")
		  	}
		  		
		  

		  
	}
